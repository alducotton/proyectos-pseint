//Ingresar 2 números y determinar cuál es el mayor de los dos.
Algoritmo Ejercicio_02_Condicionales

	Definir numeroUno Como Entero;
	Definir numeroDos Como Entero;

	Mostrar "Ingresar el primer número";
	Leer numeroUno 
	
	Mostrar "Ingresar un segundo número";
	Leer numeroDos
	
	Si numeroUno>numeroDos Entonces
		Mostrar "El mayor es el primer numero";
	SiNo
		Mostrar "El mayor es el segundo numero";
	FinSi
	
FinAlgoritmo
